<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<body>
<table border="1">
    <tr>
        <th>Id</th>
        <th>Nom Entreprise</th>
        <th>Nom Stage</th>
        <th>Description</th>
        <th>Modifier</th>
        <th>Supprimer</th>
    </tr>
    {% for c in OffreStage %}
        <tr>
            <td>{{ c.id }}</td>
            <td>{{ c.NomEntreprise }}</td>
            <td>{{ c.NomStage }}</td>
            <td>{{ c.DescriptionStage }}</td>
            <td><a href="{{ path('updateoffre',{'id':c.id}) }}">Modifier</a></td>
            <td><a href="{{ path('deleteoffre',{'id':c.id}) }}">Supprimer</a></td>


        </tr>
    {% endfor %}
</table>
</body>
</html>